 exo 1
 
 REP 1
 SELECT titre
 FROM REPRESENTATION;
 
 REP 2
 SELECT titre,lieu
 FROM REPRESENTATION;
 
 
 exo 2
 
 REP 1 
 SELECT Nom,COUNT(id)
 FROM ETUDIANT;
 
 REP 2
 SELECT MAX(note) as notemax,MIN(note) as notemin
 FROM EVALUER;
 
 
 REP 3
 SELECT AVG(note),id_Etudiant,id_Matiere
 FROM EVALUER;
 
 REP 4
 SELECT AVG(note),id_Matiere
 FROM EVALUER;
